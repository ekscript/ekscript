#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/himu/my_code/ekscript/ekscript/node_modules/.pnpm/uuid@3.4.0/node_modules/uuid/bin/uuid/node_modules:/home/himu/my_code/ekscript/ekscript/node_modules/.pnpm/uuid@3.4.0/node_modules/uuid/bin/node_modules:/home/himu/my_code/ekscript/ekscript/node_modules/.pnpm/uuid@3.4.0/node_modules/uuid/node_modules:/home/himu/my_code/ekscript/ekscript/node_modules/.pnpm/uuid@3.4.0/node_modules:/home/himu/my_code/ekscript/ekscript/node_modules/.pnpm/node_modules:/home/himu/my_code/ekscript/ekscript/node_modules:/home/himu/my_code/ekscript/node_modules:/home/himu/my_code/node_modules:/home/himu/node_modules:/home/node_modules:/node_modules:/home/himu/my_code/ekscript/ekscript/node_modules/.pnpm/teeny-request@6.0.1/node_modules/uuid/bin/uuid/node_modules:/home/himu/my_code/ekscript/ekscript/node_modules/.pnpm/teeny-request@6.0.1/node_modules/uuid/bin/node_modules:/home/himu/my_code/ekscript/ekscript/node_modules/.pnpm/teeny-request@6.0.1/node_modules/uuid/node_modules:/home/himu/my_code/ekscript/ekscript/node_modules/.pnpm/teeny-request@6.0.1/node_modules:/home/himu/my_code/ekscript/ekscript/node_modules/.pnpm/teeny-request@6.0.1/node_modules/teeny-request/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../uuid/bin/uuid" "$@"
else
  exec node  "$basedir/../../../uuid/bin/uuid" "$@"
fi
