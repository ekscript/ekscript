#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/himu/my_code/ekscript/ekscript/node_modules/.pnpm/strip-indent@1.0.1/node_modules/strip-indent/cli.js/node_modules:/home/himu/my_code/ekscript/ekscript/node_modules/.pnpm/strip-indent@1.0.1/node_modules/strip-indent/node_modules:/home/himu/my_code/ekscript/ekscript/node_modules/.pnpm/strip-indent@1.0.1/node_modules:/home/himu/my_code/ekscript/ekscript/node_modules/.pnpm/node_modules:/home/himu/my_code/ekscript/ekscript/node_modules:/home/himu/my_code/ekscript/node_modules:/home/himu/my_code/node_modules:/home/himu/node_modules:/home/node_modules:/node_modules:/home/himu/my_code/ekscript/ekscript/node_modules/.pnpm/redent@1.0.0/node_modules/strip-indent/cli.js/node_modules:/home/himu/my_code/ekscript/ekscript/node_modules/.pnpm/redent@1.0.0/node_modules/strip-indent/node_modules:/home/himu/my_code/ekscript/ekscript/node_modules/.pnpm/redent@1.0.0/node_modules:/home/himu/my_code/ekscript/ekscript/node_modules/.pnpm/redent@1.0.0/node_modules/redent/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../strip-indent/cli.js" "$@"
else
  exec node  "$basedir/../../../strip-indent/cli.js" "$@"
fi
