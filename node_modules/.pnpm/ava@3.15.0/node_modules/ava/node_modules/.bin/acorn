#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/himu/my_code/ekscript/ekscript/node_modules/.pnpm/acorn@8.0.5/node_modules/acorn/bin/acorn/node_modules:/home/himu/my_code/ekscript/ekscript/node_modules/.pnpm/acorn@8.0.5/node_modules/acorn/bin/node_modules:/home/himu/my_code/ekscript/ekscript/node_modules/.pnpm/acorn@8.0.5/node_modules/acorn/node_modules:/home/himu/my_code/ekscript/ekscript/node_modules/.pnpm/acorn@8.0.5/node_modules:/home/himu/my_code/ekscript/ekscript/node_modules/.pnpm/node_modules:/home/himu/my_code/ekscript/ekscript/node_modules:/home/himu/my_code/ekscript/node_modules:/home/himu/my_code/node_modules:/home/himu/node_modules:/home/node_modules:/node_modules:/home/himu/my_code/ekscript/ekscript/node_modules/.pnpm/ava@3.15.0/node_modules/acorn/bin/acorn/node_modules:/home/himu/my_code/ekscript/ekscript/node_modules/.pnpm/ava@3.15.0/node_modules/acorn/bin/node_modules:/home/himu/my_code/ekscript/ekscript/node_modules/.pnpm/ava@3.15.0/node_modules/acorn/node_modules:/home/himu/my_code/ekscript/ekscript/node_modules/.pnpm/ava@3.15.0/node_modules:/home/himu/my_code/ekscript/ekscript/node_modules/.pnpm/ava@3.15.0/node_modules/ava/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../acorn/bin/acorn" "$@"
else
  exec node  "$basedir/../../../acorn/bin/acorn" "$@"
fi
