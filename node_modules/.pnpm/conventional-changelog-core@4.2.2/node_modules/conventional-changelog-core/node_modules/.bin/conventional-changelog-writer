#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/himu/my_code/ekscript/ekscript/node_modules/.pnpm/conventional-changelog-writer@4.1.0/node_modules/conventional-changelog-writer/cli.js/node_modules:/home/himu/my_code/ekscript/ekscript/node_modules/.pnpm/conventional-changelog-writer@4.1.0/node_modules/conventional-changelog-writer/node_modules:/home/himu/my_code/ekscript/ekscript/node_modules/.pnpm/conventional-changelog-writer@4.1.0/node_modules:/home/himu/my_code/ekscript/ekscript/node_modules/.pnpm/node_modules:/home/himu/my_code/ekscript/ekscript/node_modules:/home/himu/my_code/ekscript/node_modules:/home/himu/my_code/node_modules:/home/himu/node_modules:/home/node_modules:/node_modules:/home/himu/my_code/ekscript/ekscript/node_modules/.pnpm/conventional-changelog-core@4.2.2/node_modules/conventional-changelog-writer/cli.js/node_modules:/home/himu/my_code/ekscript/ekscript/node_modules/.pnpm/conventional-changelog-core@4.2.2/node_modules/conventional-changelog-writer/node_modules:/home/himu/my_code/ekscript/ekscript/node_modules/.pnpm/conventional-changelog-core@4.2.2/node_modules:/home/himu/my_code/ekscript/ekscript/node_modules/.pnpm/conventional-changelog-core@4.2.2/node_modules/conventional-changelog-core/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../conventional-changelog-writer/cli.js" "$@"
else
  exec node  "$basedir/../../../conventional-changelog-writer/cli.js" "$@"
fi
