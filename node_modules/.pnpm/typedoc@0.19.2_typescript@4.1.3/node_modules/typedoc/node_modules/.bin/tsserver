#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/himu/my_code/ekscript/ekscript/node_modules/.pnpm/typescript@4.1.3/node_modules/typescript/bin/tsserver/node_modules:/home/himu/my_code/ekscript/ekscript/node_modules/.pnpm/typescript@4.1.3/node_modules/typescript/bin/node_modules:/home/himu/my_code/ekscript/ekscript/node_modules/.pnpm/typescript@4.1.3/node_modules/typescript/node_modules:/home/himu/my_code/ekscript/ekscript/node_modules/.pnpm/typescript@4.1.3/node_modules:/home/himu/my_code/ekscript/ekscript/node_modules/.pnpm/node_modules:/home/himu/my_code/ekscript/ekscript/node_modules:/home/himu/my_code/ekscript/node_modules:/home/himu/my_code/node_modules:/home/himu/node_modules:/home/node_modules:/node_modules:/home/himu/my_code/ekscript/ekscript/node_modules/.pnpm/typedoc@0.19.2_typescript@4.1.3/node_modules/typescript/bin/tsserver/node_modules:/home/himu/my_code/ekscript/ekscript/node_modules/.pnpm/typedoc@0.19.2_typescript@4.1.3/node_modules/typescript/bin/node_modules:/home/himu/my_code/ekscript/ekscript/node_modules/.pnpm/typedoc@0.19.2_typescript@4.1.3/node_modules/typescript/node_modules:/home/himu/my_code/ekscript/ekscript/node_modules/.pnpm/typedoc@0.19.2_typescript@4.1.3/node_modules:/home/himu/my_code/ekscript/ekscript/node_modules/.pnpm/typedoc@0.19.2_typescript@4.1.3/node_modules/typedoc/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../typescript/bin/tsserver" "$@"
else
  exec node  "$basedir/../../../typescript/bin/tsserver" "$@"
fi
