#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/himu/my_code/ekscript/ekscript/node_modules/.pnpm/handlebars@4.7.6/node_modules/handlebars/bin/handlebars/node_modules:/home/himu/my_code/ekscript/ekscript/node_modules/.pnpm/handlebars@4.7.6/node_modules/handlebars/bin/node_modules:/home/himu/my_code/ekscript/ekscript/node_modules/.pnpm/handlebars@4.7.6/node_modules/handlebars/node_modules:/home/himu/my_code/ekscript/ekscript/node_modules/.pnpm/handlebars@4.7.6/node_modules:/home/himu/my_code/ekscript/ekscript/node_modules/.pnpm/node_modules:/home/himu/my_code/ekscript/ekscript/node_modules:/home/himu/my_code/ekscript/node_modules:/home/himu/my_code/node_modules:/home/himu/node_modules:/home/node_modules:/node_modules:/home/himu/my_code/ekscript/ekscript/node_modules/.pnpm/typedoc@0.19.2_typescript@4.1.3/node_modules/handlebars/bin/handlebars/node_modules:/home/himu/my_code/ekscript/ekscript/node_modules/.pnpm/typedoc@0.19.2_typescript@4.1.3/node_modules/handlebars/bin/node_modules:/home/himu/my_code/ekscript/ekscript/node_modules/.pnpm/typedoc@0.19.2_typescript@4.1.3/node_modules/handlebars/node_modules:/home/himu/my_code/ekscript/ekscript/node_modules/.pnpm/typedoc@0.19.2_typescript@4.1.3/node_modules:/home/himu/my_code/ekscript/ekscript/node_modules/.pnpm/typedoc@0.19.2_typescript@4.1.3/node_modules/typedoc/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../handlebars/bin/handlebars" "$@"
else
  exec node  "$basedir/../../../handlebars/bin/handlebars" "$@"
fi
