#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/himu/my_code/ekscript/ekscript/node_modules/.pnpm/is-docker@2.1.1/node_modules/is-docker/cli.js/node_modules:/home/himu/my_code/ekscript/ekscript/node_modules/.pnpm/is-docker@2.1.1/node_modules/is-docker/node_modules:/home/himu/my_code/ekscript/ekscript/node_modules/.pnpm/is-docker@2.1.1/node_modules:/home/himu/my_code/ekscript/ekscript/node_modules/.pnpm/node_modules:/home/himu/my_code/ekscript/ekscript/node_modules:/home/himu/my_code/ekscript/node_modules:/home/himu/my_code/node_modules:/home/himu/node_modules:/home/node_modules:/node_modules:/home/himu/my_code/ekscript/ekscript/node_modules/.pnpm/is-wsl@2.2.0/node_modules/is-docker/cli.js/node_modules:/home/himu/my_code/ekscript/ekscript/node_modules/.pnpm/is-wsl@2.2.0/node_modules/is-docker/node_modules:/home/himu/my_code/ekscript/ekscript/node_modules/.pnpm/is-wsl@2.2.0/node_modules:/home/himu/my_code/ekscript/ekscript/node_modules/.pnpm/is-wsl@2.2.0/node_modules/is-wsl/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../is-docker/cli.js" "$@"
else
  exec node  "$basedir/../../../is-docker/cli.js" "$@"
fi
