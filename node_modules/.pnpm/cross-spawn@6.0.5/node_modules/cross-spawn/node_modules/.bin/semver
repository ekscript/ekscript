#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/himu/my_code/ekscript/ekscript/node_modules/.pnpm/semver@5.7.1/node_modules/semver/bin/semver/node_modules:/home/himu/my_code/ekscript/ekscript/node_modules/.pnpm/semver@5.7.1/node_modules/semver/bin/node_modules:/home/himu/my_code/ekscript/ekscript/node_modules/.pnpm/semver@5.7.1/node_modules/semver/node_modules:/home/himu/my_code/ekscript/ekscript/node_modules/.pnpm/semver@5.7.1/node_modules:/home/himu/my_code/ekscript/ekscript/node_modules/.pnpm/node_modules:/home/himu/my_code/ekscript/ekscript/node_modules:/home/himu/my_code/ekscript/node_modules:/home/himu/my_code/node_modules:/home/himu/node_modules:/home/node_modules:/node_modules:/home/himu/my_code/ekscript/ekscript/node_modules/.pnpm/cross-spawn@6.0.5/node_modules/semver/bin/semver/node_modules:/home/himu/my_code/ekscript/ekscript/node_modules/.pnpm/cross-spawn@6.0.5/node_modules/semver/bin/node_modules:/home/himu/my_code/ekscript/ekscript/node_modules/.pnpm/cross-spawn@6.0.5/node_modules/semver/node_modules:/home/himu/my_code/ekscript/ekscript/node_modules/.pnpm/cross-spawn@6.0.5/node_modules:/home/himu/my_code/ekscript/ekscript/node_modules/.pnpm/cross-spawn@6.0.5/node_modules/cross-spawn/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../semver/bin/semver" "$@"
else
  exec node  "$basedir/../../../semver/bin/semver" "$@"
fi
