#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/himu/my_code/ekscript/ekscript/node_modules/.pnpm/typescript@4.1.3/node_modules/typescript/bin/tsc/node_modules:/home/himu/my_code/ekscript/ekscript/node_modules/.pnpm/typescript@4.1.3/node_modules/typescript/bin/node_modules:/home/himu/my_code/ekscript/ekscript/node_modules/.pnpm/typescript@4.1.3/node_modules/typescript/node_modules:/home/himu/my_code/ekscript/ekscript/node_modules/.pnpm/typescript@4.1.3/node_modules:/home/himu/my_code/ekscript/ekscript/node_modules/.pnpm/node_modules:/home/himu/my_code/ekscript/ekscript/node_modules:/home/himu/my_code/ekscript/node_modules:/home/himu/my_code/node_modules:/home/himu/node_modules:/home/node_modules:/node_modules:/home/himu/my_code/ekscript/ekscript/node_modules/.pnpm/@typescript-eslint/parser@4.14.1_eslint@7.19.0+typescript@4.1.3/node_modules/typescript/bin/tsc/node_modules:/home/himu/my_code/ekscript/ekscript/node_modules/.pnpm/@typescript-eslint/parser@4.14.1_eslint@7.19.0+typescript@4.1.3/node_modules/typescript/bin/node_modules:/home/himu/my_code/ekscript/ekscript/node_modules/.pnpm/@typescript-eslint/parser@4.14.1_eslint@7.19.0+typescript@4.1.3/node_modules/typescript/node_modules:/home/himu/my_code/ekscript/ekscript/node_modules/.pnpm/@typescript-eslint/parser@4.14.1_eslint@7.19.0+typescript@4.1.3/node_modules:/home/himu/my_code/ekscript/ekscript/node_modules/.pnpm/@typescript-eslint/node_modules:/home/himu/my_code/ekscript/ekscript/node_modules/.pnpm/@typescript-eslint/parser@4.14.1_eslint@7.19.0+typescript@4.1.3/node_modules/@typescript-eslint/parser/node_modules:/home/himu/my_code/ekscript/ekscript/node_modules/.pnpm/@typescript-eslint/parser@4.14.1_eslint@7.19.0+typescript@4.1.3/node_modules/@typescript-eslint/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../typescript/bin/tsc" "$@"
else
  exec node  "$basedir/../../../../typescript/bin/tsc" "$@"
fi
