#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/himu/my_code/ekscript/ekscript/node_modules/.pnpm/conventional-recommended-bump@6.0.11/node_modules/conventional-recommended-bump/cli.js/node_modules:/home/himu/my_code/ekscript/ekscript/node_modules/.pnpm/conventional-recommended-bump@6.0.11/node_modules/conventional-recommended-bump/node_modules:/home/himu/my_code/ekscript/ekscript/node_modules/.pnpm/conventional-recommended-bump@6.0.11/node_modules:/home/himu/my_code/ekscript/ekscript/node_modules/.pnpm/node_modules:/home/himu/my_code/ekscript/ekscript/node_modules:/home/himu/my_code/ekscript/node_modules:/home/himu/my_code/node_modules:/home/himu/node_modules:/home/node_modules:/node_modules:/home/himu/my_code/ekscript/ekscript/node_modules/.pnpm/standard-version@9.1.0/node_modules/conventional-recommended-bump/cli.js/node_modules:/home/himu/my_code/ekscript/ekscript/node_modules/.pnpm/standard-version@9.1.0/node_modules/conventional-recommended-bump/node_modules:/home/himu/my_code/ekscript/ekscript/node_modules/.pnpm/standard-version@9.1.0/node_modules:/home/himu/my_code/ekscript/ekscript/node_modules/.pnpm/standard-version@9.1.0/node_modules/standard-version/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../conventional-recommended-bump/cli.js" "$@"
else
  exec node  "$basedir/../../../conventional-recommended-bump/cli.js" "$@"
fi
