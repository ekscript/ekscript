#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/himu/my_code/ekscript/ekscript/node_modules/.pnpm/semver@7.3.4/node_modules/semver/bin/semver.js/node_modules:/home/himu/my_code/ekscript/ekscript/node_modules/.pnpm/semver@7.3.4/node_modules/semver/bin/node_modules:/home/himu/my_code/ekscript/ekscript/node_modules/.pnpm/semver@7.3.4/node_modules/semver/node_modules:/home/himu/my_code/ekscript/ekscript/node_modules/.pnpm/semver@7.3.4/node_modules:/home/himu/my_code/ekscript/ekscript/node_modules/.pnpm/node_modules:/home/himu/my_code/ekscript/ekscript/node_modules:/home/himu/my_code/ekscript/node_modules:/home/himu/my_code/node_modules:/home/himu/node_modules:/home/node_modules:/node_modules:/home/himu/my_code/ekscript/ekscript/node_modules/.pnpm/standard-version@9.1.0/node_modules/semver/bin/semver.js/node_modules:/home/himu/my_code/ekscript/ekscript/node_modules/.pnpm/standard-version@9.1.0/node_modules/semver/bin/node_modules:/home/himu/my_code/ekscript/ekscript/node_modules/.pnpm/standard-version@9.1.0/node_modules/semver/node_modules:/home/himu/my_code/ekscript/ekscript/node_modules/.pnpm/standard-version@9.1.0/node_modules:/home/himu/my_code/ekscript/ekscript/node_modules/.pnpm/standard-version@9.1.0/node_modules/standard-version/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../semver/bin/semver.js" "$@"
else
  exec node  "$basedir/../../../semver/bin/semver.js" "$@"
fi
