#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/himu/my_code/ekscript/ekscript/node_modules/.pnpm/nyc@15.1.0/node_modules/nyc/bin/nyc.js/node_modules:/home/himu/my_code/ekscript/ekscript/node_modules/.pnpm/nyc@15.1.0/node_modules/nyc/bin/node_modules:/home/himu/my_code/ekscript/ekscript/node_modules/.pnpm/nyc@15.1.0/node_modules/nyc/node_modules:/home/himu/my_code/ekscript/ekscript/node_modules/.pnpm/nyc@15.1.0/node_modules:/home/himu/my_code/ekscript/ekscript/node_modules/.pnpm/node_modules:/home/himu/my_code/ekscript/ekscript/node_modules:/home/himu/my_code/ekscript/node_modules:/home/himu/my_code/node_modules:/home/himu/node_modules:/home/node_modules:/node_modules:/home/himu/my_code/ekscript/ekscript/node_modules/.pnpm/@istanbuljs/nyc-config-typescript@1.0.1_nyc@15.1.0+ts-node@9.1.1/node_modules/nyc/bin/nyc.js/node_modules:/home/himu/my_code/ekscript/ekscript/node_modules/.pnpm/@istanbuljs/nyc-config-typescript@1.0.1_nyc@15.1.0+ts-node@9.1.1/node_modules/nyc/bin/node_modules:/home/himu/my_code/ekscript/ekscript/node_modules/.pnpm/@istanbuljs/nyc-config-typescript@1.0.1_nyc@15.1.0+ts-node@9.1.1/node_modules/nyc/node_modules:/home/himu/my_code/ekscript/ekscript/node_modules/.pnpm/@istanbuljs/nyc-config-typescript@1.0.1_nyc@15.1.0+ts-node@9.1.1/node_modules:/home/himu/my_code/ekscript/ekscript/node_modules/.pnpm/@istanbuljs/node_modules:/home/himu/my_code/ekscript/ekscript/node_modules/.pnpm/@istanbuljs/nyc-config-typescript@1.0.1_nyc@15.1.0+ts-node@9.1.1/node_modules/@istanbuljs/nyc-config-typescript/node_modules:/home/himu/my_code/ekscript/ekscript/node_modules/.pnpm/@istanbuljs/nyc-config-typescript@1.0.1_nyc@15.1.0+ts-node@9.1.1/node_modules/@istanbuljs/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../nyc/bin/nyc.js" "$@"
else
  exec node  "$basedir/../../../../nyc/bin/nyc.js" "$@"
fi
