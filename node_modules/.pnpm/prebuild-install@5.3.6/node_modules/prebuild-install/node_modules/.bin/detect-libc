#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/himu/my_code/ekscript/ekscript/node_modules/.pnpm/detect-libc@1.0.3/node_modules/detect-libc/bin/detect-libc.js/node_modules:/home/himu/my_code/ekscript/ekscript/node_modules/.pnpm/detect-libc@1.0.3/node_modules/detect-libc/bin/node_modules:/home/himu/my_code/ekscript/ekscript/node_modules/.pnpm/detect-libc@1.0.3/node_modules/detect-libc/node_modules:/home/himu/my_code/ekscript/ekscript/node_modules/.pnpm/detect-libc@1.0.3/node_modules:/home/himu/my_code/ekscript/ekscript/node_modules/.pnpm/node_modules:/home/himu/my_code/ekscript/ekscript/node_modules:/home/himu/my_code/ekscript/node_modules:/home/himu/my_code/node_modules:/home/himu/node_modules:/home/node_modules:/node_modules:/home/himu/my_code/ekscript/ekscript/node_modules/.pnpm/prebuild-install@5.3.6/node_modules/detect-libc/bin/detect-libc.js/node_modules:/home/himu/my_code/ekscript/ekscript/node_modules/.pnpm/prebuild-install@5.3.6/node_modules/detect-libc/bin/node_modules:/home/himu/my_code/ekscript/ekscript/node_modules/.pnpm/prebuild-install@5.3.6/node_modules/detect-libc/node_modules:/home/himu/my_code/ekscript/ekscript/node_modules/.pnpm/prebuild-install@5.3.6/node_modules:/home/himu/my_code/ekscript/ekscript/node_modules/.pnpm/prebuild-install@5.3.6/node_modules/prebuild-install/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../detect-libc/bin/detect-libc.js" "$@"
else
  exec node  "$basedir/../../../detect-libc/bin/detect-libc.js" "$@"
fi
