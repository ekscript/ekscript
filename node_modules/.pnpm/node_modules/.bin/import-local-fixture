#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/himu/my_code/ekscript/ekscript/node_modules/.pnpm/import-local@3.0.2/node_modules/import-local/fixtures/node_modules:/home/himu/my_code/ekscript/ekscript/node_modules/.pnpm/import-local@3.0.2/node_modules/import-local/node_modules:/home/himu/my_code/ekscript/ekscript/node_modules/.pnpm/import-local@3.0.2/node_modules:/home/himu/my_code/ekscript/ekscript/node_modules/.pnpm/node_modules:/home/himu/my_code/ekscript/ekscript/node_modules:/home/himu/my_code/ekscript/node_modules:/home/himu/my_code/node_modules:/home/himu/node_modules:/home/node_modules:/node_modules:/home/himu/my_code/ekscript/ekscript/node_modules/.pnpm/node_modules/import-local/fixtures/node_modules:/home/himu/my_code/ekscript/ekscript/node_modules/.pnpm/node_modules/import-local/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../import-local/fixtures/cli.js" "$@"
else
  exec node  "$basedir/../import-local/fixtures/cli.js" "$@"
fi
