#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/himu/my_code/ekscript/ekscript/node_modules/.pnpm/@babel/parser@7.12.11/node_modules/@babel/parser/bin/node_modules:/home/himu/my_code/ekscript/ekscript/node_modules/.pnpm/@babel/parser@7.12.11/node_modules/@babel/parser/node_modules:/home/himu/my_code/ekscript/ekscript/node_modules/.pnpm/@babel/parser@7.12.11/node_modules/@babel/node_modules:/home/himu/my_code/ekscript/ekscript/node_modules/.pnpm/@babel/parser@7.12.11/node_modules:/home/himu/my_code/ekscript/ekscript/node_modules/.pnpm/@babel/node_modules:/home/himu/my_code/ekscript/ekscript/node_modules/.pnpm/node_modules:/home/himu/my_code/ekscript/ekscript/node_modules:/home/himu/my_code/ekscript/node_modules:/home/himu/my_code/node_modules:/home/himu/node_modules:/home/node_modules:/node_modules:/home/himu/my_code/ekscript/ekscript/node_modules/.pnpm/node_modules/@babel/parser/bin/node_modules:/home/himu/my_code/ekscript/ekscript/node_modules/.pnpm/node_modules/@babel/parser/node_modules:/home/himu/my_code/ekscript/ekscript/node_modules/.pnpm/node_modules/@babel/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@babel/parser/bin/babel-parser.js" "$@"
else
  exec node  "$basedir/../@babel/parser/bin/babel-parser.js" "$@"
fi
