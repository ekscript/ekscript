#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/himu/my_code/ekscript/ekscript/node_modules/.pnpm/shelljs@0.8.4/node_modules/shelljs/bin/node_modules:/home/himu/my_code/ekscript/ekscript/node_modules/.pnpm/shelljs@0.8.4/node_modules/shelljs/node_modules:/home/himu/my_code/ekscript/ekscript/node_modules/.pnpm/shelljs@0.8.4/node_modules:/home/himu/my_code/ekscript/ekscript/node_modules/.pnpm/node_modules:/home/himu/my_code/ekscript/ekscript/node_modules:/home/himu/my_code/ekscript/node_modules:/home/himu/my_code/node_modules:/home/himu/node_modules:/home/node_modules:/node_modules:/home/himu/my_code/ekscript/ekscript/node_modules/.pnpm/node_modules/shelljs/bin/node_modules:/home/himu/my_code/ekscript/ekscript/node_modules/.pnpm/node_modules/shelljs/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../shelljs/bin/shjs" "$@"
else
  exec node  "$basedir/../shelljs/bin/shjs" "$@"
fi
