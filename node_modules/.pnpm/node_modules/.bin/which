#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/himu/my_code/ekscript/ekscript/node_modules/.pnpm/which@1.3.1/node_modules/which/bin/node_modules:/home/himu/my_code/ekscript/ekscript/node_modules/.pnpm/which@1.3.1/node_modules/which/node_modules:/home/himu/my_code/ekscript/ekscript/node_modules/.pnpm/which@1.3.1/node_modules:/home/himu/my_code/ekscript/ekscript/node_modules/.pnpm/node_modules:/home/himu/my_code/ekscript/ekscript/node_modules:/home/himu/my_code/ekscript/node_modules:/home/himu/my_code/node_modules:/home/himu/node_modules:/home/node_modules:/node_modules:/home/himu/my_code/ekscript/ekscript/node_modules/.pnpm/node_modules/which/bin/node_modules:/home/himu/my_code/ekscript/ekscript/node_modules/.pnpm/node_modules/which/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../which/bin/which" "$@"
else
  exec node  "$basedir/../which/bin/which" "$@"
fi
