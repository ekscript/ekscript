#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/himu/my_code/ekscript/ekscript/node_modules/.pnpm/jsesc@2.5.2/node_modules/jsesc/bin/jsesc/node_modules:/home/himu/my_code/ekscript/ekscript/node_modules/.pnpm/jsesc@2.5.2/node_modules/jsesc/bin/node_modules:/home/himu/my_code/ekscript/ekscript/node_modules/.pnpm/jsesc@2.5.2/node_modules/jsesc/node_modules:/home/himu/my_code/ekscript/ekscript/node_modules/.pnpm/jsesc@2.5.2/node_modules:/home/himu/my_code/ekscript/ekscript/node_modules/.pnpm/node_modules:/home/himu/my_code/ekscript/ekscript/node_modules:/home/himu/my_code/ekscript/node_modules:/home/himu/my_code/node_modules:/home/himu/node_modules:/home/node_modules:/node_modules:/home/himu/my_code/ekscript/ekscript/node_modules/.pnpm/@babel/generator@7.12.11/node_modules/jsesc/bin/jsesc/node_modules:/home/himu/my_code/ekscript/ekscript/node_modules/.pnpm/@babel/generator@7.12.11/node_modules/jsesc/bin/node_modules:/home/himu/my_code/ekscript/ekscript/node_modules/.pnpm/@babel/generator@7.12.11/node_modules/jsesc/node_modules:/home/himu/my_code/ekscript/ekscript/node_modules/.pnpm/@babel/generator@7.12.11/node_modules:/home/himu/my_code/ekscript/ekscript/node_modules/.pnpm/@babel/node_modules:/home/himu/my_code/ekscript/ekscript/node_modules/.pnpm/@babel/generator@7.12.11/node_modules/@babel/generator/node_modules:/home/himu/my_code/ekscript/ekscript/node_modules/.pnpm/@babel/generator@7.12.11/node_modules/@babel/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../jsesc/bin/jsesc" "$@"
else
  exec node  "$basedir/../../../../jsesc/bin/jsesc" "$@"
fi
