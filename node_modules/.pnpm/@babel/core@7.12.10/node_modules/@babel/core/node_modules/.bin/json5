#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/himu/my_code/ekscript/ekscript/node_modules/.pnpm/json5@2.2.0/node_modules/json5/lib/cli.js/node_modules:/home/himu/my_code/ekscript/ekscript/node_modules/.pnpm/json5@2.2.0/node_modules/json5/lib/node_modules:/home/himu/my_code/ekscript/ekscript/node_modules/.pnpm/json5@2.2.0/node_modules/json5/node_modules:/home/himu/my_code/ekscript/ekscript/node_modules/.pnpm/json5@2.2.0/node_modules:/home/himu/my_code/ekscript/ekscript/node_modules/.pnpm/node_modules:/home/himu/my_code/ekscript/ekscript/node_modules:/home/himu/my_code/ekscript/node_modules:/home/himu/my_code/node_modules:/home/himu/node_modules:/home/node_modules:/node_modules:/home/himu/my_code/ekscript/ekscript/node_modules/.pnpm/@babel/core@7.12.10/node_modules/json5/lib/cli.js/node_modules:/home/himu/my_code/ekscript/ekscript/node_modules/.pnpm/@babel/core@7.12.10/node_modules/json5/lib/node_modules:/home/himu/my_code/ekscript/ekscript/node_modules/.pnpm/@babel/core@7.12.10/node_modules/json5/node_modules:/home/himu/my_code/ekscript/ekscript/node_modules/.pnpm/@babel/core@7.12.10/node_modules:/home/himu/my_code/ekscript/ekscript/node_modules/.pnpm/@babel/node_modules:/home/himu/my_code/ekscript/ekscript/node_modules/.pnpm/@babel/core@7.12.10/node_modules/@babel/core/node_modules:/home/himu/my_code/ekscript/ekscript/node_modules/.pnpm/@babel/core@7.12.10/node_modules/@babel/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../json5/lib/cli.js" "$@"
else
  exec node  "$basedir/../../../../json5/lib/cli.js" "$@"
fi
