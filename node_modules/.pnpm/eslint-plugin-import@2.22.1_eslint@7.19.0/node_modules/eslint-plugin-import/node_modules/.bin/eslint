#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/himu/my_code/ekscript/ekscript/node_modules/.pnpm/eslint@7.19.0/node_modules/eslint/bin/eslint.js/node_modules:/home/himu/my_code/ekscript/ekscript/node_modules/.pnpm/eslint@7.19.0/node_modules/eslint/bin/node_modules:/home/himu/my_code/ekscript/ekscript/node_modules/.pnpm/eslint@7.19.0/node_modules/eslint/node_modules:/home/himu/my_code/ekscript/ekscript/node_modules/.pnpm/eslint@7.19.0/node_modules:/home/himu/my_code/ekscript/ekscript/node_modules/.pnpm/node_modules:/home/himu/my_code/ekscript/ekscript/node_modules:/home/himu/my_code/ekscript/node_modules:/home/himu/my_code/node_modules:/home/himu/node_modules:/home/node_modules:/node_modules:/home/himu/my_code/ekscript/ekscript/node_modules/.pnpm/eslint-plugin-import@2.22.1_eslint@7.19.0/node_modules/eslint/bin/eslint.js/node_modules:/home/himu/my_code/ekscript/ekscript/node_modules/.pnpm/eslint-plugin-import@2.22.1_eslint@7.19.0/node_modules/eslint/bin/node_modules:/home/himu/my_code/ekscript/ekscript/node_modules/.pnpm/eslint-plugin-import@2.22.1_eslint@7.19.0/node_modules/eslint/node_modules:/home/himu/my_code/ekscript/ekscript/node_modules/.pnpm/eslint-plugin-import@2.22.1_eslint@7.19.0/node_modules:/home/himu/my_code/ekscript/ekscript/node_modules/.pnpm/eslint-plugin-import@2.22.1_eslint@7.19.0/node_modules/eslint-plugin-import/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../eslint/bin/eslint.js" "$@"
else
  exec node  "$basedir/../../../eslint/bin/eslint.js" "$@"
fi
