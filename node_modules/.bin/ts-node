#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/himu/my_code/ekscript/ekscript/node_modules/.pnpm/ts-node@9.1.1_typescript@4.1.3/node_modules/ts-node/dist/node_modules:/home/himu/my_code/ekscript/ekscript/node_modules/.pnpm/ts-node@9.1.1_typescript@4.1.3/node_modules/ts-node/node_modules:/home/himu/my_code/ekscript/ekscript/node_modules/.pnpm/ts-node@9.1.1_typescript@4.1.3/node_modules:/home/himu/my_code/ekscript/ekscript/node_modules/.pnpm/node_modules:/home/himu/my_code/ekscript/ekscript/node_modules:/home/himu/my_code/ekscript/node_modules:/home/himu/my_code/node_modules:/home/himu/node_modules:/home/node_modules:/node_modules:/home/himu/my_code/ekscript/ekscript/node_modules/ts-node/dist/node_modules:/home/himu/my_code/ekscript/ekscript/node_modules/ts-node/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../ts-node/dist/bin.js" "$@"
else
  exec node  "$basedir/../ts-node/dist/bin.js" "$@"
fi
