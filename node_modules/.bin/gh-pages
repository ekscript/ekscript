#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/himu/my_code/ekscript/ekscript/node_modules/.pnpm/gh-pages@3.1.0/node_modules/gh-pages/bin/node_modules:/home/himu/my_code/ekscript/ekscript/node_modules/.pnpm/gh-pages@3.1.0/node_modules/gh-pages/node_modules:/home/himu/my_code/ekscript/ekscript/node_modules/.pnpm/gh-pages@3.1.0/node_modules:/home/himu/my_code/ekscript/ekscript/node_modules/.pnpm/node_modules:/home/himu/my_code/ekscript/ekscript/node_modules:/home/himu/my_code/ekscript/node_modules:/home/himu/my_code/node_modules:/home/himu/node_modules:/home/node_modules:/node_modules:/home/himu/my_code/ekscript/ekscript/node_modules/gh-pages/bin/node_modules:/home/himu/my_code/ekscript/ekscript/node_modules/gh-pages/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../gh-pages/bin/gh-pages.js" "$@"
else
  exec node  "$basedir/../gh-pages/bin/gh-pages.js" "$@"
fi
