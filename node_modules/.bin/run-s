#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/himu/my_code/ekscript/ekscript/node_modules/.pnpm/npm-run-all@4.1.5/node_modules/npm-run-all/bin/run-s/node_modules:/home/himu/my_code/ekscript/ekscript/node_modules/.pnpm/npm-run-all@4.1.5/node_modules/npm-run-all/bin/node_modules:/home/himu/my_code/ekscript/ekscript/node_modules/.pnpm/npm-run-all@4.1.5/node_modules/npm-run-all/node_modules:/home/himu/my_code/ekscript/ekscript/node_modules/.pnpm/npm-run-all@4.1.5/node_modules:/home/himu/my_code/ekscript/ekscript/node_modules/.pnpm/node_modules:/home/himu/my_code/ekscript/ekscript/node_modules:/home/himu/my_code/ekscript/node_modules:/home/himu/my_code/node_modules:/home/himu/node_modules:/home/node_modules:/node_modules:/home/himu/my_code/ekscript/ekscript/node_modules/npm-run-all/bin/run-s/node_modules:/home/himu/my_code/ekscript/ekscript/node_modules/npm-run-all/bin/node_modules:/home/himu/my_code/ekscript/ekscript/node_modules/npm-run-all/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../npm-run-all/bin/run-s/index.js" "$@"
else
  exec node  "$basedir/../npm-run-all/bin/run-s/index.js" "$@"
fi
